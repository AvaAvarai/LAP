; Fibonacci sequence demo
; Shows recursion and conditional logic

(define (fib n)
  (if (<= n 1)
      n
      (+ (fib (- n 1)) (fib (- n 2)))))

(println "Fibonacci sequence:")
(println "fib(0) = " (fib 0))
(println "fib(1) = " (fib 1))
(println "fib(2) = " (fib 2))
(println "fib(3) = " (fib 3))
(println "fib(4) = " (fib 4))
(println "fib(5) = " (fib 5))
(println "fib(6) = " (fib 6))
(println "fib(7) = " (fib 7))